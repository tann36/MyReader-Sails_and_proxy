<!DOCTYPE html>
<!--[if IE 8]> 				 <html class="no-js lt-ie9" lang="en" > <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en" > <!--<![endif]-->

<head>
	<meta charset="utf-8" />
  <meta name="viewport" content="width=device-width" />
  <title>MyReader</title>
  
  
<script src="/linker/js/vendor/custom.modernizr.js"></script>
<script src="/linker/js/vendor/jquery.js"></script>
<script src="/linker/js/handlebars.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/backbone.js/1.0.0/backbone-min.js"></script>
  
<link rel="stylesheet" href="/linker/styles/foundation.css">
<link rel="stylesheet" href="/linker/styles/normalize.css">
<link rel="stylesheet" href="/linker/styles/app.css">
	


</head>

<body>




<!-- Header -->
<div class="contain-to-grid fixed">
	<nav class="top-bar">
		<ul class="title-area">
			<li class="name"><h1><a href="/solr">MyReader</a></h1></li>
			<li class="toggle-topbar"><a href><span>Search</span></a></li>
		</ul>
		<section class="top-bar-section">
			<ul class="right">
				<li class="search has-form" >
					<form id="searchForm">
						<div class="row">
							<div class="small-8 columns">
								<input type="text" name="searchTerm" />
							</div>
							<div class="small-4 columns">
								<input class="button" name="srch" type="submit" value="Search" />
							</div>
						</div>
					</form>
				</li>
			</ul>
		</section>
	</nav>
</div>

<!-- Content -->
<div id="main">
	<div class="row">
        <div class="large-12 columns">
			<h3 class="sectionTitle cyan" id="ftTitle">Readability</h3>
			<div class="slideshow-wrapper">
			<ul id="newsOrbit" data-orbit class="orbit-slides-container">	</ul>
			</div>
			<hr>
		</div>
	</div>
		<div class="row" id="categories">
	<!--
		<div draggable="true" class="large-6 small-12 columns" ondragover= "allowDrop(event)" ondrop="drop(event)" ondragstart="drag(event)" id="marketing"></div>
		<div draggable="true" class="large-6 small-12 columns" ondragover= "allowDrop(event)" ondrop="drop(event)" ondragstart="drag(event)" id="startup"></div>
		<div draggable="true" class="large-6 small-12 columns" ondragover= "allowDrop(event)" ondrop="drop(event)" ondragstart="drag(event)" id="cloud"></div>
		<div draggable="true" class="large-6 small-12 columns" id="gadget"></div>
		<div draggable="true" class="large-6 small-12 columns" id="test"></div>
		<div draggable="true" class="large-6 small-12 columns" id="filtered"></div>
		<div draggable="true" class="large-6 small-12 columns" id="technology"></div>
		<div draggable="true" class="large-6 small-12 columns" id="featured"></div>  
	-->
	</div>
	
	
	
	<footer class="black">
		<div class="row footnotes">
			<div class="small-12 columns">
				<h3>Please note that this is beta version and contains bugs.</h3>
				<h3>To return section to its' original form (small or hidden), click colored header.</h3>
			</div>
		</div>
	</footer>
	
	
	
	<script>
		document.write('<script src=' +
		('__proto__' in {} ? 'js/vendor/zepto' : 'js/vendor/jquery') +
		'.js><\/script>')
	</script>
	
	
	<script src="/linker/js/foundation.min.js"></script>
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
	<script src="/linker/js/foundation/foundation.orbit.js"></script>
	<script src="/linker/js/foundation/foundation.dropdown.js"></script>
	
	<script src="/linker/js/script.js"></script>

	<script src="js/script.js">
		//$(document).foundation();  
		<!--$(document).foundation('orbit', 'start'); -->
	</script>
	
	
<div id="articleModal" class="reveal-modal"></div>
</body>
</html>
